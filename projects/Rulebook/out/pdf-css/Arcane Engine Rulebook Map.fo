                                <fo:table-row page-break-inside="avoid">
                                    <fo:table-cell start-indent="0" end-indent="0" font-family="sans-serif, Verdana, helvetica" font-size="0.75em" number-rows-spanned="6" border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1pt" border-left-color="rgb(238 , 238 , 238)" border-right-color="black" border-right-style="solid" border-right-width="1pt" border-top-color="rgb(238 , 238 , 238)" text-align="left" display-align="center"><fo:block>Strength</fo:block></fo:table-cell>
